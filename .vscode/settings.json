{
    "todo-tree.highlights.defaultHighlight": {
        "icon": "alert",
        "type": "text-and-comment",
        "foreground": "black",
        "background": "white",
        "opacity": 50,
        "iconColour": "blue",
        "gutterIcon": true,
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "note",
            "foreground": "black",
            "background": "gray",
            "iconColour": "white",
        },
        "NOTE": {
            "icon": "note",
            "foreground": "white",
            "background": "gray",
            "iconColour": "gray",
        },
        "COMMENT": {
            "icon": "note",
            "foreground": "white",
            "background": "gray",
            "iconColour": "gray",
        },
        "FIXME": {
            "foreground": "black",
            "background": "yellow",
            "iconColour": "yellow",
        },
        "BUG": {
            "foreground": "black",
            "background": "red",
            "iconColour": "red",
        },
        "[ ]": {
            "icon": "check",
            "foreground": "black",
            "background": "white",
            "iconColour": "yellow",
        },
        "[x]": {
            "icon": "check",
            "foreground": "white",
            "background": "green",
            "iconColour": "green",
        }
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "NOTE",
        "COMMENT",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*",
    "editor.fontFamily": "Monaspace ArgonBold Italic",
    "editor.fontSize": 14,
    "editor.fontWeight": 600,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": true,
    "explorer.expandSingleFolderWorkspaces": true,
    "explorer.compactFolders": false,
    "scm.inputFontFamily": "'Monaspace Neon', monospace",
    "chat.editor.fontWeight": "600",
    "terminal.integrated.fontWeight": "600",
    "editor.inlineSuggest.fontFamily": "'Monaspace Neon', monospace",
    "debug.console.fontFamily": "'Monaspace Neon', monospace",
    "terminal.integrated.fontFamily": "monospace",
    "editor.codeLensFontFamily": "'Monaspace Neon', monospace",
    "chat.editor.fontFamily": "'Monaspace Neon', monospace",
    "notebook.output.fontFamily": "'Monaspace Neon', monospace",
    "editor.fontLigatures": false,
    "editor.fontVariations": false,
    "editor.experimentalWhitespaceRendering": "font",
}